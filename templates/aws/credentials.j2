# {{ ansible_managed }}
{% for profile in item.value.aws.profiles %}
[{{ profile.name }}]
{%  if profile.aws_access_key_id | default(false) %}
aws_access_key_id = {{ profile.aws_access_key_id }}
{%  endif %}
{%  if profile.aws_secret_access_key | default(false) %}
aws_secret_access_key = {{ profile.aws_secret_access_key }}
{%  endif %}
{% endfor %}
